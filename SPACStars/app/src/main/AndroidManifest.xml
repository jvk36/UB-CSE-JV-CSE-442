<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication">

        <activity android:name=".ShowListing" />
        <activity android:name=".Alerts" />
        <activity android:name=".CategoryList"></activity>
        <activity android:name=".WebSearch" />
        <activity android:name=".SearchSocialMedia" />
        <activity android:name=".SPACLivePricesMain" />
        <activity android:name=".SPACTopDailyPriceChangeMain" />
        <activity android:name=".SPACBottomDailyPriceChangeMain" />
        <activity android:name=".SPACTopWeeklyPriceChangeMain" />
        <activity android:name=".SPACBottomWeeklyPriceChangeMain" />
        <activity android:name=".SPACTopMonthlyPriceChangeMain" />
        <activity android:name=".SPACBottomMonthlyPriceChangeMain" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".MainActivity">
          
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".Alerts$timedNotification" >
        </receiver>

    </application>
</manifest>